{
	"name": "@inseefr/lunatic",
	"version": "2.0.2-v2",
	"workersVersion": "0.2.4-experimental",
	"description": "Library of questionnaire components",
	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/Lunatic.git"
	},
	"author": "INSEE (http://www.insee.fr)",
	"contributor": [
		{
			"name": "Laurent Caouissin",
			"email": "laurent.caouissin@agriculture.gouv.fr"
		},
		{
			"name": "Renaud Genevois",
			"email": "renaud.genevois@insee.fr"
		},
		{
			"name": "Nicolas Laval",
			"email": "nicolas.laval@insee.fr"
		}
	],
	"license": "MIT",
	"main": "lib/index.js",
	"module": "src/index.js",
	"files": [
		"lib"
	],
	"scripts": {
		"test": "cross-env NODE_PATH=src/ jest",
		"test-watch": "cross-env NODE_PATH=src/ jest --watchAll",
		"test-coverage": "cross-env NODE_PATH=src/ jest --coverage",
		"build": "babel src --out-dir lib --copy-files",
		"prepublish": "npm test && npm run build",
		"build-append-worker": "webpack --config ./webpack-append-worker.config.js",
		"build-searching-worker": "webpack --config ./webpack-searching-worker.config.js",
		"build-label-worker": "webpack --config ./webpack-label-worker.config.js",
		"build-workers": "yarn build-append-worker && yarn build-searching-worker && yarn build-label-worker",
		"storybook": "start-storybook -p 9999",
		"build-storybook": "build-storybook"
	},
	"keywords": [
		"react",
		"component",
		"react-component",
		"library"
	],
	"dependencies": {
		"@inseefr/trevas": "^0.1.16",
		"@inseefr/vtl-2.0-antlr-tools": "^0.1.0-bundle",
		"antlr4": "4.8.0",
		"classnames": "^2.3.1",
		"date-fns": "^2.25.0",
		"lodash.camelcase": "^4.3.0",
		"lodash.debounce": "^4.0.8",
		"lodash.isequal": "^4.5.0",
		"object-hash": "^2.2.0",
		"prop-types": "^15.7.2",
		"react-keyboard-event-handler": "^1.5.4",
		"react-markdown": "^8.0.3",
		"react-number-format": "^5.0.0-beta.4",
		"react-tooltip": "^4.2.15",
		"remove-accents": "^0.4.2",
		"sass": "^1.32.2",
		"snowball": "^0.3.1",
		"string-tokenizer": "^0.0.8"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.6",
		"@babel/core": "^7.18.6",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/plugin-proposal-private-property-in-object": "^7.18.6",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-transform-modules-commonjs": "^7.9.0",
		"@babel/plugin-transform-react-jsx": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.0",
		"@emotion/react": "^11.9.3",
		"@emotion/styled": "^11.9.3",
		"@mui/material": "^5.8.6",
		"@storybook/addon-a11y": "^6.5.12",
		"@storybook/addon-actions": "^6.5.8",
		"@storybook/addon-essentials": "^6.5.8",
		"@storybook/addon-interactions": "^6.5.8",
		"@storybook/addon-links": "^6.5.8",
		"@storybook/builder-webpack5": "^6.5.8",
		"@storybook/manager-webpack5": "^6.5.8",
		"@storybook/react": "^6.5.8",
		"@storybook/testing-library": "^0.0.11",
		"@testing-library/react-hooks": "^7.0.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"core-js": "^3.17.3",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.0",
		"damerau-levenshtein": "^1.0.7",
		"eslint": "7.30.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^4.0.8",
		"file-loader": "^6.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.0.6",
		"jest-sonar-reporter": "^2.0.0",
		"jest-transform-stub": "^2.0.0",
		"jsdom": "^16.6.0",
		"jsdom-global": "^3.0.2",
		"postcss": "^8.2.6",
		"postcss-scss": "^3.0.4",
		"prettier": "^2.0.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"regenerator-runtime": "^0.13.7",
		"remove-accents": "^0.4.2",
		"rxjs-marbles": "^6.0.1",
		"sass-loader": "^10.0.3",
		"source-map-explorer": "^2.4.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!src/**/index.js",
			"!src/**/*.stories.js"
		],
		"testURL": "http://localhost",
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "identity-obj-proxy"
		},
		"transform": {
			"^.+\\.js$": "babel-jest",
			".+\\.(png|jpg)$": "jest-transform-stub"
		},
		"coverageProvider": "v8",
		"testResultsProcessor": "jest-sonar-reporter",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"testEnvironment": "jsdom"
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": {
		"singleQuote": true,
		"semi": true,
		"useTabs": true,
		"bracketSpacing": true,
		"trailingComma": "es5"
	},
	"peerDependencies": {
		"react": "^17.0.2"
	}
}
