{
	"name": "@inseefr/lunatic",
	"version": "0.1.0-experimental",
	"description": "Library of questionnaire components",
	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/Lunatic.git"
	},
	"author": "INSEE (http://www.insee.fr)",
	"contributor": [
		{
			"name": "Nicolas Laval",
			"email": "nicolas.laval@insee.fr"
		}
	],
	"license": "MIT",
	"main": "lib/index.js",
	"module": "src/index.js",
	"files": [
		"lib",
		"src"
	],
	"scripts": {
		"test": "cross-env NODE_PATH=src/ jest",
		"test-watch": "cross-env NODE_PATH=src/ jest --watchAll",
		"test-coverage": "cross-env NODE_PATH=src/ jest --coverage",
		"start-storybook": "cross-env NODE_PATH=src/ start-storybook -s ./public -p 9999",
		"build-storybook": "cross-env NODE_PATH=src/ build-storybook -o built-storybook/storybook",
		"build": "rollup -c rollup.config.js",
		"prepublish": "npm test && npm run build",
		"analyze": "yarn build && source-map-explorer lib/index.js --html bundle-report/lunatic.html"
	},
	"keywords": [
		"react",
		"component",
		"react-component",
		"library"
	],
	"dependencies": {
		"@inseefr/trevas": "^0.1.4",
		"lodash.camelcase": "^4.3.0",
		"lodash.debounce": "^4.0.8",
		"lodash.isequal": "^4.5.0",
		"prop-types": "^15.7.2", 
		"react-markdown": "^5.0.3",
		"react-tooltip": "^4.2.15",
		"remove-accents": "^0.4.2",
		"sass": "^1.32.2"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-syntax-dynamic-import": "^7.7.4",
		"@babel/plugin-transform-modules-commonjs": "^7.9.0",
		"@babel/plugin-transform-react-jsx": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.0",
		"@storybook/addon-a11y": "^5.2.8",
		"@storybook/addon-knobs": "^5.2.8",
		"@storybook/addon-options": "^5.2.8",
		"@storybook/addon-viewport": "^5.2.8",
		"@storybook/react": "^5.2.8",
		"@testing-library/react-hooks": "^7.0.0",
		"@timdp/rollup-plugin-image": "^1.0.3",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"classnames": "^2.2.5",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.0",
		"enzyme": "^3.7.0",
		"eslint": "7.30.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^4.0.8",
		"file-loader": "^6.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.0.6",
		"jest-sonar-reporter": "^2.0.0",
		"jest-transform-stub": "^2.0.0",
		"jsdom": "^16.6.0",
		"jsdom-global": "^3.0.2",
		"postcss": "^8.2.6",
		"postcss-scss": "^3.0.4",
		"prettier": "^2.0.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rollup": "^2.52.8",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-babel-minify": "^10.0.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-terser": "^7.0.1",
		"rollup-plugin-uglify": "^6.0.3",
		"rollup-plugin-url": "^3.0.1",
		"rollup-watch": "^4.3.1",
		"rxjs-marbles": "^6.0.1",
		"sass-loader": "^10.0.3",
		"source-map-explorer": "^2.4.2",
		"storybook-addon-jsx": "^7.2.3",
		"storybook-readme": "^5.0.8",
		"style-loader": "^2.0.0",
		"url-loader": "^4.1.0"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}",
			"!src/**/index.js",
			"!src/**/*.stories.js"
		],
		"testURL": "http://localhost",
		"moduleNameMapper": {
			"^.+\\.(css|less|scss)$": "identity-obj-proxy"
		},
		"transform": {
			"^.+\\.js$": "babel-jest",
			".+\\.(png|jpg)$": "jest-transform-stub"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/src/tests/setup/setupTests.js"
		],
		"coverageProvider": "v8",
		"testResultsProcessor": "jest-sonar-reporter",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"testEnvironment": "jsdom"
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": {
		"singleQuote": true,
		"semi": true,
		"useTabs": true,
		"bracketSpacing": true,
		"trailingComma": "es5"
	},
	"peerDependencies": {
		"react": "^17.0.2"
	}
}
