	"repository": {
		"type": "git",
		"url": "https://github.com/InseeFr/Lunatic.git"
	},
	"author": "INSEE (http://www.insee.fr)",
	"contributor": [
		{
			"name": "Jonathan Boyer",
			"email": "contact@grafikart.fr"
