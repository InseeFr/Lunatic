    "react-number-format": "^5.1.3",
    "react-tooltip": "^5.23.0",
    "sass": "^1.58.3"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@playwright/test": "^1.30.0",
    "@storybook/addon-a11y": "^8.0.5",
    "@storybook/addon-actions": "^8.0.5",
