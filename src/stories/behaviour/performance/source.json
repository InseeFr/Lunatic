      "name": "BIRTHDAY",
      "values": {
        "PREVIOUS": [null],
        "COLLECTED": [null],
        "FORCED": [null],
        "EDITED": [null],
        "INPUTED": [null]
      }
    },
    {
      "variableType": "COLLECTED",
      "name": "AGE",
      "values": {
        "PREVIOUS": [null],
        "COLLECTED": [null],
        "FORCED": [null],
