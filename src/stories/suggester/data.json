{
	"suggesters": {
		"naf-rev2": {
			"name": "naf-rev2",
			"url": "https://inseefr.github.io/Lunatic/storybook/naf-rev2.json",
			"fields": [
				{
					"name": "label",
					"rules": ["[\\w]+"],
					"language": "French",
					"min": 2
				},
				{ "name": "id" }
			],
			"queryParser": {
				"type": "tokenized",
				"params": { "language": "French", "pattern": "[\\w.]+" }
			},
			"version": "1"
		},
		"cog-communes": {
			"name": "cog-communes",
			"url": "https://inseefr.github.io/Lunatic/storybook/communes-2019.json",
			"fields": [
				{ "name": "label", "rules": "soft" },
				{ "name": "nccenr", "rules": "soft" },
				{ "name": "id", "rules": "soft" }
			],
			"queryParser": { "type": "soft" },
			"version": "1"
		},
		"cog-tokenized": {
			"name": "cog-tokenized",
			"url": "https://inseefr.github.io/Lunatic/storybook/communes-2019.json",
			"fields": [
				 
				{ "name": "label", "rules": ["[\\w]+"],
					"language": "French",
					"stemmer": false,
					"min": 2 }, 
				{ "name": "id", "rules": "soft" }
			],
			"stopWords": [],
			"queryParser": {
				"type": "tokenized",
				"params": { "language": "French", "pattern": "[\\w.]+", "stemmer": false }
			},
			"version": "1"
		}
	},
	"components": [
		{
			"id": "sugg",
			"componentType": "Suggester",
			"mandatory": false,
			"label": "Naf rev. 2",
			"path": "data/cities",
			"storeName": "naf-rev2",
			"response": {
				"name": "CITY"
			}
		},
		{
			"id": "sugg-communes",
			"componentType": "Suggester",
			"mandatory": false,
			"label": "French municipalities",
			"path": "data/communes",
			"storeName": "cog-communes",
			"response": {
				"name": "COMMUNES"
			}
		},
		{
			"id": "sugg-cog-tokenized",
			"componentType": "Suggester",
			"mandatory": false,
			"label": "French municipalities with tokenized label",
			"path": "data/communes-tok",
			"storeName": "cog-tokenized",
			"response": {
				"name": "COMMUNES"
			}
		}
	],
	"variables": [
		{
			"variableType": "COLLECTED",
			"name": "CITY",
			"componentRef": "sugg",
			"values": {
				"PREVIOUS": null,
				"COLLECTED": null,
				"FORCED": null,
				"EDITED": null,
				"INPUTED": null
			}
		},
		{
			"variableType": "COLLECTED",
			"name": "COMMUNES",
			"componentRef": "sugg-communes",
			"values": {
				"PREVIOUS": null,
				"COLLECTED": null,
				"FORCED": null,
				"EDITED": null,
				"INPUTED": null
			}
		}
	]
}
