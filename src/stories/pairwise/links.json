			"maxPage": "1",
			"conditionFilter": { "value": "true", "type": "VTL" },
			"loopDependencies": ["PRENOM"],
			"components": [
				{
					"componentType": "Textarea",
					"page": "4.1",
					"label": {
						"value": "PRENOM || \", why do you live without your parents?\"",
						"type": "VTL|MD"
					},
					"conditionFilter": {
						"value": "\"3\" not_in LINKS and AGE < 18",
						"type": "VTL"
					},
					"id": "why",
					"response": {
						"name": "WHY"
					}
				}
