				{
					"componentType": "Textarea",

					"label": {
						"value": "\"Pouvez-vous décrire le lien entre \" || xAxis || \" et \" || yAxis || \" ?\"",
						"type": "VTL|MD"
					},
					"conditionFilter": {
						"value": "LINKS = \"17\"",
						"type": "VTL"
					},
					"id": "other",
					"response": {
						"name": "OTHER"
					}
				}
			]
		},
		{
			"id": "pariwise-block",
			"componentType": "Loop",
			"paginatedLoop": true,
