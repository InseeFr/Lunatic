					"conditionFilter": { "value": "xAxis <> yAxis", "type": "VTL" },
					"label": {
						"value": "\"Quel est le lien entre \" || xAxis || \" et \" || yAxis || \" ?\"",
						"type": "VTL|MD"
					},
					"response": {
						"name": "LINKS"
					},
					"options": [
						{ "value": "1", "label": "\"Conjoint, Conjointe\"" },
						{ "value": "2", "label": "\"Mère, père\"" },
						{ "value": "3", "label": "\"Fille, fils\"" },
						{
							"value": "4",
							"label": "\"Soeur, demi-soeur, quasi-soeur, Frére, demi-frére, qusi-frére\""
						},
						{ "value": "5", "label": "\"Grand-mère, Grand-père\"" },
						{ "value": "6", "label": "\"Petite-fille, petit-fils\"" },
						{ "value": "7", "label": "\"Tante, Oncle\"" },
						{ "value": "8", "label": "\"Nièce, neveu\"" },
						{ "value": "9", "label": "\"Cousine, cousin\"" },

						{
							"value": "10",
							"label": "\"Belle-mère, beau-père (conjoint.e d'un des parents)\""
						},
						{
							"value": "11",
							"label": "\"Belle-mère, beau-père (enfant du conjoint)\""
						},
						{
							"value": "12",
							"label": "\"Belle-fille, beau-fils (enfant du conjoint)\""
						},
						{
							"value": "13",
							"label": "\"Belle-fille, beau-fils (conjoint.e d'un enfant')\""
						},
						{ "value": "14", "label": "\"Enfant placé en famille d'accueil\"" },
						{ "value": "15", "label": "\"Colocataire, sous-locataire\"" },
						{ "value": "16", "label": "\"Belle-soeur, beau-frère\"" },

						{ "value": "17", "label": "\"Autre\"" }
					]
