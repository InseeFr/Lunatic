{
	"maxPage": "3",
	"components": [
		{
			"id": "seq",
			"componentType": "Sequence",
			"label": "\"Description des individus de votre logement\"",
			"conditionFilter": { "value": "true" },
			"page": "1"
		},
		{
			"id": "loop-prenom",
			"componentType": "RosterForLoop",
			"label": "\"Ajouter un individu\"",
			"conditionFilter": { "value": "true" },
			"bindingDependencies": ["PRENOM"],
			"lines": { "min": { "value": 1 }, "max": { "value": "10" } },
			"page": "1",
			"components": [
				{
					"componentType": "Input",
					"label": "Prénom",
					"conditionFilter": { "value": "true" },
					"maxLength": 30,
					"bindingDependencies": ["PRENOM"],
					"id": "prenom",
					"response": {
						"name": "PRENOM"
					}
				}
			]
		},
		{
			"id": "pariwise-links",
			"componentType": "PairwiseLinks",
			"conditionFilter": { "value": "true" },
			"xAxis": { "value": "count(PRENOM)", "type": "VTL" },
			"yAxis": { "value": "count(PRENOM)", "type": "VTL" },
			"page": "2",
			"components": [
				{
					"componentType": "Radio",
					"conditionFilter": { "value": "xAxis = yAxis" },
					"label": "\"Quel est le lien entre \" || xAxis || \" et \" || yAxis || \" ?\"",
					"response": {
						"name": "LINKS"
					},
					"options": [
						{ "value": "1", "label": "Conjoint" },
						{ "value": "2", "label": "Mère, père" },
						{ "value": "3", "label": "Fille, fils" },
						{
							"value": "4",
							"label": "Belle-mère, beau-père, conjoint du parent"
						},
						{
							"value": "5",
							"label": "Belle-mère, beau-père, parent du conjoint"
						},
						{ "value": "6", "label": "Enfant du conjoint" },
						{ "value": "7", "label": "Conjoint d’un enfant" },
						{ "value": "8", "label": "Sœur, frère (y compris demi et quasi)" },
						{ "value": "9", "label": "Grand-parent" },
						{ "value": "10", "label": "Petit-enfant" },
						{ "value": "11", "label": "Tante, oncle" },
						{ "value": "12", "label": "Cousin(e)" },
						{ "value": "13", "label": "Nièce, neveu" },
						{ "value": "14", "label": "Belle-sœur, beau-frère" },
						{ "value": "15", "label": "Enfant placé" },
						{ "value": "16", "label": "Autre" }
					]
				}
			]
		},
		{
			"id": "pariwise-block",
			"componentType": "PairwiseBlock",
			"conditionFilter": { "value": "true" },
			"page": "3"
		}
	],
	"variables": [
		{
			"variableType": "COLLECTED",
			"name": "PRENOM",
			"componentRef": "loop-prenom",
			"values": {
				"PREVIOUS": [null],
				"COLLECTED": [null],
				"FORCED": [null],
				"EDITED": [null],
				"INPUTED": [null]
			}
		},
		{
			"variableType": "COLLECTED",
			"name": "LINKS",
			"componentRef": "loop-prenom",
			"values": {
				"PREVIOUS": [[null]],
				"COLLECTED": [[null]],
				"FORCED": [[null]],
				"EDITED": [[null]],
				"INPUTED": [[null]]
			}
		},
		{
			"variableType": "CALCULATED",
			"name": "xAxis",
			"expression": "PRENOM",
			"bindingDependencies": ["PRENOM"],
			"shapeFrom": "PRENOM",
			"inFilter": "false"
		},
		{
			"variableType": "CALCULATED",
			"name": "yAxis",
			"expression": "PRENOM",
			"bindingDependencies": ["PRENOM"],
			"shapeFrom": "PRENOM",
			"inFilter": "false"
		}
	]
}
