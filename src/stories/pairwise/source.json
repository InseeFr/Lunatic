				"EDITED": [[null]],
				"INPUTED": [[null]]
			}
		},
		{
			"variableType": "COLLECTED",
			"name": "OTHER",
			"values": {
				"PREVIOUS": [[null]],
				"COLLECTED": [[null]],
				"FORCED": [[null]],
				"EDITED": [[null]],
				"INPUTED": [[null]]
			}
		},
		{
