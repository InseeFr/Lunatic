				"FORCED": [null],
				"EDITED": [null],
				"INPUTED": [null]
			}
		},
		{
			"variableType": "CALCULATED",
			"name": "xAxis",
			"expression": { "value": "PRENOM", "type": "VTL" },
			"bindingDependencies": ["PRENOM"],
			"shapeFrom": "PRENOM",
			"inFilter": "false"
		},
		{
			"variableType": "CALCULATED",
			"name": "yAxis",
			"expression": { "value": "PRENOM", "type": "VTL" },
