{
	"id": "loop_menage",
	"componentType": "Loop",
	"iterations": "count(MENAGE)",
	"conditionFilter": { "value": "true" },
	"loopDependencies": ["MENAGE"],
	"bindingDependencies": [
		"MENAGE",
		"FAMILLE",
		"INDIV",
		"NB_VEHICULE",
		"AGE",
		"PERMIS",
		"POINTS"
	],
	"components": [
		{
			"id": "seq_2",
			"componentType": "Sequence",
			"label": "\"Ménage : \" || MENAGE",
			"conditionFilter": { "value": "true" },
			"bindingDependencies": ["MENAGE"]
		},
		{
			"id": "roster_famille",
			"componentType": "RosterForLoop",
			"mandatory": true,
			"positioning": "HORIZONTAL",
			"label": "\"Qui sont les familles du ménage \" || MENAGE || \" ?\"",
			"conditionFilter": { "value": "true" },
			"bindingDependencies": ["MENAGE", "FAMILLE"],
			"lines": { "min": 1, "max": 10 },
			"headers": [{ "headerCell": true, "label": "Famille" }],
			"components": [
				{
					"id": "input_famille",
					"componentType": "Input",
					"conditionFilter": { "value": "true" },
					"maxLength": 30,
					"bindingDependencies": ["FAMILLE"],
					"response": {
						"name": "FAMILLE"
					}
				}
			]
		},
		{
			"id": "loop_famille",
			"componentType": "Loop",
			"iterations": "count(FAMILLE)",
			"conditionFilter": { "value": "true" },
			"loopDependencies": ["FAMILLE"],
			"bindingDependencies": [
				"MENAGE",
				"FAMILLE",
				"NB_VEHICULE",
				"INDIV",
				"AGE",
				"PERMIS",
				"POINTS"
			],
			"components": [
				{
					"id": "seq_3",
					"componentType": "Sequence",
					"label": "\"Famille : \" || FAMILLE",
					"conditionFilter": { "value": "true" },
					"bindingDependencies": ["FAMILLE"]
				},
				{
					"id": "vehicule",
					"label": "\"Famille \" || FAMILLE ||\", de combien de véhicule disposez-vous ?\"",
					"componentType": "InputNumber",
					"conditionFilter": { "value": "true" },
					"bindingDependencies": ["FAMILLE", "NB_VEHICULE"],
					"mandatory": false,
					"min": 0,
					"max": 20,
					"decimals": 0,
					"response": {
						"name": "NB_VEHICULE"
					}
				},
				{
					"id": "roster_indiv",
					"componentType": "RosterForLoop",
					"mandatory": true,
					"positioning": "HORIZONTAL",
					"label": "\"Qui sont les individus de la famille \" || FAMILLE || \" ?\"",
					"conditionFilter": { "value": "true" },
					"bindingDependencies": ["MENAGE", "FAMILLE", "INDIV"],
					"lines": { "min": 1, "max": 10 },
					"headers": [{ "headerCell": true, "label": "Individu" }],
					"components": [
						{
							"id": "input_indiv",
							"componentType": "Input",
							"conditionFilter": { "value": "true" },
							"maxLength": 30,
							"bindingDependencies": ["INDIV"],
							"response": {
								"name": "INDIV"
							}
						}
					]
				},
				{
					"id": "loop_indiv",
					"componentType": "Loop",
					"iterations": "count(INDIV)",
					"conditionFilter": { "value": "true" },
					"loopDependencies": ["INDIV"],
					"bindingDependencies": [
						"MENAGE",
						"FAMILLE",
						"INDIV",
						"AGE",
						"PERMIS",
						"POINTS"
					],
					"components": [
						{
							"id": "seq_4",
							"componentType": "Sequence",
							"label": "\"Individu : \" || INDIV",
							"conditionFilter": { "value": "true" },
							"bindingDependencies": ["INDIV"]
						},
						{
							"id": "age",
							"label": "INDIV || \", quel est votre âge ?\"",
							"componentType": "InputNumber",
							"conditionFilter": { "value": "true" },
							"bindingDependencies": ["INDIV", "AGE"],
							"mandatory": false,
							"min": 0,
							"max": 120,
							"decimals": 0,
							"response": {
								"name": "AGE"
							}
						},
						{
							"id": "permis",
							"label": "INDIV || \", avez-vous le permis de conduire ?\"",
							"componentType": "CheckboxOne",
							"conditionFilter": "if(cast(AGE, integer)>=18) then \"normal\" else \"hidden\"",
							"bindingDependencies": ["INDIV", "PERMIS"],
							"mandatory": false,
							"response": {
								"name": "PERMIS"
							},
							"options": [
								{ "value": "1", "label": "Oui" },
								{ "value": "0", "label": "Non" }
							]
						},
						{
							"id": "points",
							"label": "INDIV || \", avez-vous 12 points sur votre permis de conduire ?\"",
							"componentType": "Radio",
							"conditionFilter": "if(cast(PERMIS, string) = \"1\") then \"normal\" else \"hidden\"",
							"bindingDependencies": ["INDIV", "POINTS"],
							"mandatory": false,
							"response": {
								"name": "POINTS"
							},
							"options": [
								{ "value": "1", "label": "Oui" },
								{ "value": "0", "label": "Non" }
							]
						}
					]
				}
			]
		}
	]
}
