{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://insee.fr/lunatic/survey.schema.json",
	"title": "Lunatic Source",
	"description": "A lunatic survey unit",
	"type": "object",
	"properties": {
		"label": {
			"$ref": "#/$defs/VTLExpression"
		},
		"pagination": {
			"enum": ["question", "sequence"]
		},
		"enoCoreVersion": { "type": "string" },
		"lunaticModelVersion": { "type": "string" },
		"generatingDate": { "type": "string" },
		"modele": { "type": "string" },
		"id": { "type": "string" },
		"components": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/ComponentDefinitionWithPage"
			}
		},
		"variables": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/Variable"
			}
		},
		"suggesters": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/SuggesterDefinition"
			}
		},
		"cleaning": {
			"type": "object",
			"additionalProperties": {
				"type": "object",
				"additionalProperties": {
					"type": "string"
				}
			}
		},
		"missingBlock": {
			"type": "object",
			"additionalProperties": {
				"type": "array",
				"items": {
					"type": "string"
				}
			}
		},
		"resizing": {
			"type": "object",
			"additionalProperties": {
				"oneOf": [
					{
						"type": "object",
						"properties": {
							"size": {
								"type": "string",
								"description": "VTL Expression for size."
							},
							"variables": {
								"type": "array",
								"items": {
									"type": "string"
								}
							}
						},
						"required": ["size", "variables"]
					},
					{
						"type": "object",
						"properties": {
							"sizeForLinksVariables": {
								"oneOf": [
									{
										"type": "array",
										"tsType": "[string, string]",
										"prefixItems": [
											{
												"type": "string"
											},
											{
												"type": "string"
											}
										]
									},
									{
										"type": "object",
										"properties": {
											"xAxisSize": {
												"type": "string"
											},
											"yAxisSize": {
												"type": "string"
											}
										},
										"required": ["xAxisSize", "yAxisSize"]
									}
								]
							},
							"linksVariables": {
								"type": "array",
								"items": {
									"type": "string"
								}
							}
						},
						"required": ["sizeForLinksVariables", "linksVariables"]
					},
					{
						"type": "object",
						"properties": {
							"size": {
								"type": "string",
								"description": "VTL Expression for size."
							},
							"variables": {
								"type": "array",
								"items": {
									"type": "string"
								}
							},
							"sizeForLinksVariables": {
								"oneOf": [
									{
										"type": "array",
										"tsType": "[string, string]",
										"prefixItems": [
											{
												"type": "string"
											},
											{
												"type": "string"
											}
										]
									},
									{
										"type": "object",
										"properties": {
											"xAxisSize": {
												"type": "string"
											},
											"yAxisSize": {
												"type": "string"
											}
										},
										"required": ["xAxisSize", "yAxisSize"]
									}
								]
							},
							"linksVariables": {
								"type": "array",
								"items": {
									"type": "string"
								}
							}
						},
						"required": [
							"sizeForLinksVariables",
							"linksVariables",
							"size",
							"variables"
						]
					}
				]
			}
		},
		"maxPage": {
			"type": "string"
		},
		"fillers": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/FillerDefinition"
			}
		}
	},
	"required": ["components", "variables"],
	"$defs": {
		"VTLExpression": {
			"type": "object",
			"properties": {
				"value": {
					"description": "Valid VTL Expression",
					"type": "string"
				},
				"bindingDependencies": {
					"description": "Variables used in the expression",
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"type": {
					"type": "string",
					"enum": ["VTL", "VTL|MD", "TXT"]
				}
			},
			"required": ["value", "type"],
			"additionalProperties": false
		},
		"VTLScalarExpression": {
			"type": "object",
			"properties": {
				"value": {
					"description": "Valid VTL Expression",
					"type": "string"
				},
				"bindingDependencies": {
					"description": "Variables used in the expression",
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"type": {
					"type": "string",
					"const": "VTL"
				}
			},
			"required": ["value", "type"],
			"additionalProperties": false
		},
		"Declaration": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"declarationType": {
					"type": "string",
					"enum": [
						"INSTRUCTION",
						"COMMENT",
						"HELP",
						"CODECARD",
						"WARNING",
						"STATEMENT"
					]
				},
				"position": {
					"type": "string",
					"enum": [
						"AFTER_QUESTION_TEXT",
						"AFTER_RESPONSE",
						"BEFORE_QUESTION_TEXT",
						"DETACHABLE"
					]
				},
				"label": {
					"$ref": "#/$defs/VTLExpression"
				}
			},
			"required": ["id", "declarationType", "position", "label"],
			"additionalProperties": false
		},
		"ControlDefinition": {
			"type": "object",
			"properties": {
				"id": {
					"type": "string"
				},
				"criticality": {
					"type": "string",
					"enum": ["INFO", "WARN", "ERROR"]
				},
				"typeOfControl": {
					"type": "string",
					"enum": ["FORMAT", "CONSISTENCY"]
				},
				"control": {
					"$ref": "#/$defs/VTLExpression"
				},
				"errorMessage": {
					"$ref": "#/$defs/VTLExpression"
				},
				"bindingDependencies": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"type": {
					"type": "string",
					"enum": ["roundabout", "ROW", "simple"]
				},
				"iterations": {
					"$ref": "#/$defs/VTLScalarExpression"
				}
			},
			"required": ["id", "criticality", "control", "errorMessage"],
			"additionalProperties": false
		},
		"ResponseDefinition": {
			"type": "object",
			"properties": {
				"name": {
					"type": "string"
				}
			},
			"required": ["name"],
			"additionalProperties": false
		},
		"ComponentDefinition": {
			"discriminator": {
				"propertyName": "componentType"
			},
			"oneOf": [
				{
					"$ref": "#/$defs/ComponentInputDefinition"
				},
				{
					"$ref": "#/$defs/ComponentSequenceDefinition"
				},
				{
					"$ref": "#/$defs/ComponentRoundaboutDefinition"
				},
				{
					"$ref": "#/$defs/ComponentLoopDefinition"
				},
				{
					"$ref": "#/$defs/ComponentRosterForLoopDefinition"
				},
				{
					"$ref": "#/$defs/ComponentTableDefinition"
				},
				{
					"$ref": "#/$defs/ComponentNumberDefinition"
				},
				{
					"$ref": "#/$defs/ComponentDurationDefinition"
				},
				{
					"$ref": "#/$defs/ComponentDatePickerDefinition"
				},
				{
					"$ref": "#/$defs/ComponentCheckboxGroupDefinition"
				},
				{
					"$ref": "#/$defs/ComponentCheckboxBooleanDefinition"
				},
				{
					"$ref": "#/$defs/ComponentRadioDefinition"
				},
				{
					"$ref": "#/$defs/ComponentDropdownDefinition"
				},
				{
					"$ref": "#/$defs/ComponentQuestionDefinition"
				},
				{
					"$ref": "#/$defs/ComponentCheckboxOneDefinition"
				},
				{
					"$ref": "#/$defs/ComponentSuggesterDefinition"
				},
				{
					"$ref": "#/$defs/ComponentPairWiseLinksDefinition"
				},
				{
					"$ref": "#/$defs/ComponentSummaryDefinition"
				},
				{
					"$ref": "#/$defs/ComponentText"
				},
				{
					"$ref": "#/$defs/ComponentAccordion"
				}
			]
		},
		"ComponentDefinitionWithPage": {
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinition"
				},
				{
					"type": "object",
					"oneOf": [
						{
							"properties": {
								"page": {
									"type": "string"
								}
							},
							"required": ["page"]
						},
						{
							"properties": {
								"goToPage": {
									"type": "string"
								}
							},
							"required": ["goToPage"]
						}
					]
				}
			]
		},
		"ComponentDefinitionBase": {
			"type": "object",
			"properties": {
				"label": {
					"$ref": "#/$defs/VTLExpression"
				},
				"description": {
					"$ref": "#/$defs/VTLExpression"
				},
				"declarations": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Declaration"
					}
				},
				"conditionFilter": {
					"$ref": "#/$defs/VTLScalarExpression"
				},
				"controls": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ControlDefinition"
					}
				},
				"id": {
					"type": "string"
				},
				"mandatory": {
					"type": "boolean"
				},
				"missingResponse": {
					"$ref": "#/$defs/ResponseDefinition"
				}
			},
			"required": ["id"]
		},
		"ComponentDefinitionBaseWithResponse": {
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				},
				{
					"type": "object",
					"required": ["response"],
					"properties": {
						"response": {
							"$ref": "#/$defs/ResponseDefinition"
						}
					}
				}
			]
		},
		"ComponentInputDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"enum": ["Input", "Textarea"]
				},
				"maxLength": {
					"type": "integer"
				}
			},
			"required": ["componentType"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentSummaryDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Summary"
				},
				"sections": {
					"type": "array",
					"items": {
						"type": "object",
						"required": ["id", "title", "responses"],
						"properties": {
							"id": {
								"type": "string"
							},
							"iterations": {
								"$ref": "#/$defs/VTLExpression"
							},
							"title": {
								"$ref": "#/$defs/VTLExpression"
							},
							"responses": {
								"type": "array",
								"items": {
									"type": "object",
									"properties": {
										"id": {
											"type": "string"
										},
										"label": {
											"$ref": "#/$defs/VTLExpression"
										},
										"value": {
											"$ref": "#/$defs/VTLExpression"
										}
									},
									"required": ["id", "label", "value"]
								}
							}
						}
					}
				}
			},
			"required": ["componentType", "sections"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentSequenceDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"enum": ["Sequence", "Subsequence"]
				},
				"goToPage": {
					"type": "string"
				}
			},
			"required": ["componentType"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentRoundaboutDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Roundabout"
				},
				"iterations": {
					"$ref": "#/$defs/VTLScalarExpression"
				},
				"locked": {
					"type": "boolean"
				},
				"progressVariable": { "type": "string" },
				"item": {
					"type": "object",
					"properties": {
						"label": {
							"$ref": "#/$defs/VTLScalarExpression"
						},
						"description": {
							"$ref": "#/$defs/VTLScalarExpression"
						},
						"disabled": {
							"$ref": "#/$defs/VTLScalarExpression"
						}
					},
					"required": ["label"]
				},
				"components": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ComponentDefinitionWithPage"
					}
				}
			},
			"required": [
				"componentType",
				"components",
				"iterations",
				"locked",
				"item",
				"progressVariable"
			],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentRosterForLoopDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "RosterForLoop"
				},
				"components": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ComponentDefinition"
					}
				},
				"lines": {
					"type": "object",
					"properties": {
						"min": {
							"$ref": "#/$defs/VTLScalarExpression"
						},
						"max": {
							"$ref": "#/$defs/VTLScalarExpression"
						}
					},
					"required": ["min", "max"]
				},
				"header": {
					"$ref": "#/$defs/TableHeader"
				}
			},
			"required": ["componentType", "components", "lines"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentLoopDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Loop"
				},
				"loopDependencies": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			},
			"required": ["componentType"],
			"oneOf": [
				{
					"$ref": "#/$defs/PaginatedLoop"
				},
				{
					"$ref": "#/$defs/BlockLoop"
				}
			]
		},
		"ComponentTableDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Table"
				},
				"header": {
					"$ref": "#/$defs/TableHeader"
				},
				"body": {
					"type": "array",
					"items": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"colspan": {
									"type": "number"
								},
								"rowspan": {
									"type": "integer"
								}
							},
							"oneOf": [
								{
									"$ref": "#/$defs/ComponentDefinition"
								},
								{
									"type": "object",
									"required": ["label"],
									"properties": {
										"label": {
											"$ref": "#/$defs/VTLExpression"
										}
									},
									"not": {
										"type": "object",
										"required": ["componentType"],
										"properties": {
											"componentType": {
												"type": "string"
											}
										}
									}
								}
							]
						}
					}
				}
			},
			"required": ["componentType", "body"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentNumberDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "InputNumber"
				},
				"unit": {
					"oneOf": [
						{
							"type": "string"
						},
						{
							"$ref": "#/$defs/VTLExpression"
						}
					]
				},
				"min": {
					"type": "number"
				},
				"max": {
					"type": "number"
				},
				"decimals": {
					"type": "integer"
				}
			},
			"required": ["componentType"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentDatePickerDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Datepicker"
				},
				"dateFormat": {
					"type": "string",
					"enum": ["YYYY-MM-DD", "YYYY", "YYYY-MM"]
				},
				"min": {
					"type": "string"
				},
				"max": {
					"type": "string"
				}
			},
			"required": ["componentType", "dateFormat"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentDurationDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Duration"
				},
				"format": {
					"type": "string",
					"enum": ["PnYnM", "PTnHnM"]
				}
			},
			"required": ["componentType", "format"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentCheckboxGroupDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "CheckboxGroup"
				},
				"orientation": {
					"enum": ["horizontal", "vertical"]
				},
				"responses": {
					"type": "array",
					"items": {
						"type": "object",
						"required": ["label", "response", "id"],
						"properties": {
							"label": {
								"$ref": "#/$defs/VTLExpression"
							},
							"description": {
								"$ref": "#/$defs/VTLExpression"
							},
							"response": {
								"$ref": "#/$defs/ResponseDefinition"
							},
							"conditionFilter": {
								"$ref": "#/$defs/VTLExpression"
							},
							"id": {
								"type": "string"
							},
							"detail": {
								"type": "object",
								"properties": {
									"label": {
										"$ref": "#/$defs/VTLExpression"
									},
									"response": {
										"$ref": "#/$defs/ResponseDefinition"
									}
								},
								"required": ["response"]
							}
						}
					}
				}
			},
			"required": ["componentType", "responses"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentCheckboxBooleanDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "CheckboxBoolean"
				}
			},
			"required": ["componentType"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentRadioDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Radio"
				},
				"orientation": {
					"type": "string",
					"enum": ["horizontal", "vertical"]
				},
				"options": {
					"$ref": "#/$defs/OptionsWithDetail"
				}
			},
			"required": ["componentType", "options"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentDropdownDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Dropdown"
				},
				"options": {
					"$ref": "#/$defs/Options"
				}
			},
			"required": ["componentType", "options"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBaseWithResponse"
				}
			]
		},
		"ComponentQuestionDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "Question"
				},
				"components": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/ComponentDefinition"
					}
				}
			},
			"required": ["componentType", "components"],
			"allOf": [
				{
					"$ref": "#/$defs/ComponentDefinitionBase"
				}
			]
		},
		"ComponentCheckboxOneDefinition": {
			"type": "object",
			"properties": {
				"componentType": {
					"type": "string",
					"const": "CheckboxOne"
				},
				"options": {
					"$ref": "#/$defs/OptionsWithDetail"
				}
			},
			"required": ["componentType", "options"],
			"allOf": [
